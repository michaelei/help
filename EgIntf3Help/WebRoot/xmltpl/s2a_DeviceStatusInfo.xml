	<!-- 扩展模块状态 -->
	<ExtModuleStatusInfo>
		<ctrlUUID>12345678</ctrlUUID>
		<deviceCode>100</deviceCode>
		<deviceType>1</deviceType>
		<workStatus>0</workStatus>
