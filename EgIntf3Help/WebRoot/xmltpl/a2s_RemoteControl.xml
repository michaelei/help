<?xml version="1.0" encoding="UTF-8"?>
<egintf3 name="RemoteControl">
	<!-- 由app发送上来的xml数据一般都会携带pkgId，该Id由app自己指定，代表了这个数据包的唯一性 ，而结果返回时也会携带这个号码-->
	<pkgId>1</pkgId>
	<ctrlUUID>12345678</ctrlUUID>
	<deviceCode>100</deviceCode>
	<controlAction>0</controlAction>
	<controlParam>abc</controlParam>
	<!-- 自定义的字符串，可以省略，如手机号码等，一般用于远程开门结果的匹配 -->
	<customInfo>13511111111</customInfo>
</egintf3>