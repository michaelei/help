<?xml version="1.0" encoding="UTF-8"?>
<egintf3 name="SetupParameter">
	<!-- 由app发送上来的xml数据一般都会携带pkgId，该Id由app自己指定，代表了这个数据包的唯一性 ，而结果返回时也会携带这个号码-->
	<pkgId>1</pkgId>
	<ctrlUUID>12345678</ctrlUUID>
	<deviceCode>100</deviceCode>
	<!-- 主控器参数和门锁参数二选一，可以根据deviceCode判断 -->
	<controller>
		<!-- 以下都可选，至少一个 -->
		<matchCode>abcd</matchCode>
		<refreshInterval>0</refreshInterval>
		<serverURL>aaa</serverURL>
		<sourceMsisdn>aaa</sourceMsisdn>
		<busyTime>aa</busyTime>
		<customParam>aa</customParam>
	</controller>
	<lock>
		<!-- 以下都可选，至少一个 -->
		<areaID>0</areaID>
		<openDelay>6</openDelay>
		<pin>1234</pin>
		<pinRetry>3</pinRetry>
		<fingerCheckMode>0</fingerCheckMode>
		<fingerCheckParam>abc</fingerCheckParam>
		<allowedOpenMode>abc</allowedOpenMode>
		<fireLink>0</fireLink>
		<defenceMode>0</defenceMode>
		<paramValue>abc</paramValue>
		<buildingID>0</buildingID>
		<floorID>0</floorID>
		<soundMode>0</soundMode>
		<allowAlwaysOpen>0</allowAlwaysOpen>
		<allowLongTimeOpenedAlert>0</allowLongTimeOpenedAlert>
		<workSector>abc</workSector>
	</lock>
</egintf3>