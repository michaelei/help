<?xml version="1.0" encoding="UTF-8"?>
<egintf3 name="SetupKey">
	<!-- 由app发送上来的xml数据一般都会携带pkgId，该Id由app自己指定，代表了这个数据包的唯一性 ，而结果返回时也会携带这个号码-->
	<pkgId>1</pkgId>
	<ctrlUUID>12345678</ctrlUUID>
	<deviceCode>100</deviceCode>
	<!-- 多组密钥信息，密钥编号从1开始 -->
	<keyInfo>
		<keyNo>1</keyNo>
		<!-- 1=DES, 2=3DES, 5=AES -->
		<keyType>1</keyType>
		<!-- 16进制字符 -->
		<keyPlain>0102030405060708</keyPlain>
	</keyInfo>
	<keyInfo>
		<keyNo>2</keyNo>
		<keyType>5</keyType>
		<keyPlain>0102030405060708</keyPlain>
	</keyInfo>
</egintf3>