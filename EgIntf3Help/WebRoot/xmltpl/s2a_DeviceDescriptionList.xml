	 -->
	<DeviceDescription addr="1234" ctrlUUID="00:00:00:00:00:11" deviceCode="100" deviceType="DTYPE_Lock" name="0102030405060708" />
	<DeviceDescription addr="1235" ctrlUUID="00:00:00:00:00:11" deviceCode="101" deviceType="DTYPE_Lock" name="0102030405060708" />
</egintf3>